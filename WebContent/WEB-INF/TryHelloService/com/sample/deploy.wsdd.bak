<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TryHelloService WSDL service -->

  <service name="TryHello" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://sample.com"/>
      <parameter name="wsdlServiceElement" value="TryHelloService"/>
      <parameter name="schemaQualified" value="http://sample.com"/>
      <parameter name="wsdlServicePort" value="TryHello"/>
      <parameter name="className" value="com.sample.TryHelloSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TryHello"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="helloName" qname="operNS:helloName" xmlns:operNS="http://sample.com" returnQName="retNS:helloNameReturn" xmlns:retNS="http://sample.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:name" xmlns:pns="http://sample.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="helloName"/>

  </service>
</deployment>
